   "source": [
    "def diff_sum_squares(n):\n",
    "    sum_1 = 0\n",
    "    sum_2 = 0\n",
    "    for i in range(1, n + 1):\n",
    "        sum_1 += i\n",
    "        sum_2 += i**2\n",
    "    return sum_1**2 - sum_2"
   ]
