   "source": [
    "def even_fib(max_value):\n",
    "    curr = 2\n",
    "    last = 1\n",
    "    even_sum = 0\n",
    "    while curr < max_value:\n",
    "        if curr % 2 == 0:\n",
    "            even_sum += curr\n",
    "        last, curr = curr, curr + last\n",
    "    return even_sum"
   ]
